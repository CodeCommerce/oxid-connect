{
  "name": "makaira/connect",
  "type": "oxideshop-module",
  "require-dev": {
    "phpunit/phpunit": "^4.0",
    "pdepend/pdepend": "@stable",
    "phpmd/phpmd": "@stable",
    "squizlabs/php_codesniffer": "@stable"
  },
  "autoload": {
    "classmap": ["src/"],
    "psr-4": {
        "Makaira\\Connect\\": ["src/Makaira/Connect"]
    }
  },
  "require": {
    "makaira/shared-libs": "^2020.3.1"
  },
  "extra": {
    "oxideshop": {
      "blacklist-filter": [
        "tests/**/*",
        "phpunit.xml",
        ".gitignore",
        "changelog.txt",
        "dic.php",
        "src/Makaira/Connect/**/*"
      ],
      "target-directory": "makaira/connect"
    }
  }
}
